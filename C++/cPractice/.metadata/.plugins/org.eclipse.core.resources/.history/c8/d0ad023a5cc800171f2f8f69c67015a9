#include <iostream>
using namespace std;

void swap_v(int a,int b){
	int temp;
	temp = a;
	a = b;
	b = temp;
	cout << "*** " << a << " " << b << " ***" << endl;
}

void swap_r(int& a, int& b){
	int temp;
	temp = a;
	a = b;
	b = temp;
	cout << "*** " << a << " " << b << " ***" << endl;
}

//void swap_cr(const int& a, const int& b){
//	int temp;
//	temp = a;
//	a = b;
//	b = temp;
//} const, so can never work

int main (){
	int x = 7;
	int y = 9;

	swap_v(x,y);
	cout << x << " " << y << endl;

	const int cx = 7;
	const int cy = 9;

	swap_v(cx,cy);
	cout << cx << " " << cy << endl;


}
