//******************************* Exercise 3 & 4 ****************************************

#include<iostream>
#include<vector>

using namespace std;

void fibonacci(int x, int y, vector<int>& v, int n){

	v.push_back(x);
	v.push_back(y);

	for(int i = 2; i < n; i++){

	}


}

int main(){

   int n, first = 0, second = 1, next;

   cout << "Enter the number of terms of Fibonacci series you want" << endl;
   cin >> n;

   cout << "First " << n << " terms of Fibonacci series are :- " << endl;

   for (int c = 0 ; c < n ; c++ )
   {
      if ( c <= 1 )
         next = c;
      else
      {
         next = first + second;
         first = second;
         second = next;
      }
      cout << next << endl;
   }

   return 0;
}



